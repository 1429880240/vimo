{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "destination": "./docs/page",
    "encoding": "utf8",
    "template": "node_modules/docdash"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown"
  ],
  "markdown": {
    "tags": [
      "usage",
      "slot"
    ]
  },
  "source": {
    "include": [
      "README.md",
      "src/vimo/components/app",
      "src/vimo/components/content",
      "src/vimo/components/refresher",
      "src/vimo/components/icon",
      "src/vimo/components/grid",
      "src/vimo/components/infinite-scroll",
      "src/vimo/components/spinner",
      "src/vimo/components/tabs"
    ],
    "exclude": [
      "gulpfile.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}

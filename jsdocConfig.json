{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/minami"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "README.md",
      "src/vimo/components/content",
      "src/vimo/components/action-sheet",
      "src/vimo/components/alert",
      "src/vimo/components/app",
      "src/vimo/components/avatar",
      "src/vimo/components/backdrop",
      "src/vimo/components/badge",
      "src/vimo/components/button",
      "src/vimo/components/card",
      "src/vimo/components/grid",
      "src/vimo/components/icon",
      "src/vimo/components/img",
      "src/vimo/components/input",
      "src/vimo/components/item",
      "src/vimo/components/label",
      "src/vimo/components/list",
      "src/vimo/components/loading",
      "src/vimo/components/menu",
      "src/vimo/components/modal",
      "src/vimo/components/nav",
      "src/vimo/components/nav-controller",
      "src/vimo/components/navbar",
      "src/vimo/components/note",
      "src/vimo/components/page",
      "src/vimo/components/range",
      "src/vimo/components/searchbar",
      "src/vimo/components/segment",
      "src/vimo/components/spinner",
      "src/vimo/components/tabs",
      "src/vimo/components/thumbnail",
      "src/vimo/components/toast",
      "src/vimo/components/toggle",
      "src/vimo/components/toolbar",
      "src/vimo/components"
    ],
    "exclude":["gulpfile.js"],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}

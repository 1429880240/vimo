{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/minami"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "README.md",
      "./src/vimo/components/content"
    ],
    "exclude":["gulpfile.js"],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}

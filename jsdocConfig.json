{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/minami"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown"
  ],
  "source": {
    "includePattern": "\\.(vue)$"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}

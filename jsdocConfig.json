{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/minami"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "README.md",
      "./src/vimo/components",
      "./src/vimo/config",
      "./src/vimo/geolocation",
      "./src/vimo/log",
      "./src/vimo/platform",
      "./src/vimo/storage",
      "./src/vimo/util",
      "./src/vimo/index.js"

    ],
    "exclude":["gulpfile.js"],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}

{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "destination": "./docs/page",
    "encoding": "utf8",
    "template": "./docs/docdash"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown",
    "docs/plugins/slot.js"
  ],
  "markdown": {
    "tags": [
      "usage",
      "slot"
    ],
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include": [
      "README.md",
      "CHANGELOG.md",
      "raw/components/content"
    ],
    "exclude": [
      "gulpfile.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}

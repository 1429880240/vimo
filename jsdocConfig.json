{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/minami"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown"
  ],
  "markdown": {
    "tags": ["usage", "slot"]
  },
  "source": {
    "include": [
      "README.md",
      "src/vimo/components/content",
      "src/vimo/components/action-sheet",
      "src/vimo/components/alert",
      "src/vimo/components/app"
    ],
    "exclude":["gulpfile.js"],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}

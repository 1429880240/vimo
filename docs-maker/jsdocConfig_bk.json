{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "destination": "./docs/page",
    "encoding": "utf8",
    "template": "./docs/docdash"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "plugins/markdown"
  ],
  "markdown": {
    "tags": [
      "usage",
      "slot"
    ],
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include": [

      "README.md",
      "../dev/../dev/raw/components/action-sheet",
      "../dev/raw/components/alert",
      "../dev/raw/components/app",
      "../dev/raw/components/content",
      "../dev/raw/components/refresher",
      "../dev/raw/components/icon",
      "../dev/raw/components/grid",
      "../dev/raw/components/infinite-scroll",
      "../dev/raw/components/spinner",
      "../dev/raw/components/backdrop",
      "../dev/raw/components/badge",
      "../dev/raw/components/button",
      "../dev/raw/components/card",
      "../dev/raw/components/fab",
      "../dev/raw/components/img",
      "../dev/raw/components/input",
      "../dev/raw/components/item",
      "../dev/raw/components/toggle",
      "../dev/raw/components/label",
      "../dev/raw/components/toast",
      "../dev/raw/components/loading",
      "../dev/raw/components/modal",
      "../dev/raw/components/avatar",
      "../dev/raw/components/tabs"
    ],
    "exclude": [
      "gulpfile.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}

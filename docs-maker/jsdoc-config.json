{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "destination": "../docs",
    "encoding": "utf8",
    "template": "theme"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "node_modules/jsdoc/plugins/markdown",
    "plugins/slot.js",
    "plugins/demo.js",
    "plugins/usage.js",
    "plugins/component.js"
  ],
  "markdown": {
    "tags": [
      "usage",
      "component",
      "slot"
    ],
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include1": [
      "./theme/README.md",
      "./theme/fixtures/base",
      "./theme/fixtures/mixins",
      "./theme/fixtures/strings",
      "./theme/fixtures/tutorials",
      "./theme/fixtures/utils",
      "./theme/fixtures/documents"
    ],
    "include2": [
      "../README.md",
      "../dev/raw/util",
      "../dev/raw/components/app",
      "../dev/raw/components/content",
      "../dev/raw/components/page",
      "../dev/raw/components/nav",
      "../dev/raw/base/config.js",
      "../dev/raw/base/history.js",
      "../dev/raw/base/platform.js",
      "../dev/raw/base/platform-default-configs.js"
    ],
    "include": [
      "../dev/raw/components/content"
    ],
    "exclude": [
      "gulpfile.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}

{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "destination": "../docs",
    "encoding": "utf8",
    "template": "theme"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "node_modules/jsdoc/plugins/markdown",
    "plugins/slot.js",
    "plugins/demo.js",
    "plugins/usage.js",
    "plugins/props.js",
    "plugins/component.js"
  ],
  "markdown": {
    "tags": [
      "props",
      "usage",
      "component",
      "demo",
      "slot"
    ],
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include1": [
      "../README.md",
      "../dev/raw/util",

      "../dev/raw/base/config.js",
      "../dev/raw/base/history.js",
      "../dev/raw/base/platform.js",

      "../dev/raw/components/app",
      "../dev/raw/components/content",
      "../dev/raw/components/page",
      "../dev/raw/components/nav",

      "../dev/raw/components/segment",
      "../dev/raw/components/slides",
      "../dev/raw/components/tabs",

      "../dev/raw/components/alert",
      "../dev/raw/components/action-sheet",
      "../dev/raw/components/loading",
      "../dev/raw/components/modal",
      "../dev/raw/components/toast",
      "../dev/raw/components/indicator",

      "../dev/raw/components/infinite-scroll",
      "../dev/raw/components/refresher",

      "../dev/raw/components/list",
      "../dev/raw/components/item",
      "../dev/raw/components/avatar",
      "../dev/raw/components/thumbnail",
      "../dev/raw/components/note",

      "../dev/raw/components/label",
      "../dev/raw/components/input",
      "../dev/raw/components/checkbox",
      "../dev/raw/components/radio",
      "../dev/raw/components/range",
      "../dev/raw/components/searchbar",
      "../dev/raw/components/select",
      "../dev/raw/components/toggle",

      "../dev/raw/components/spinner",
      "../dev/raw/components/img",
      "../dev/raw/components/icon",
      "../dev/raw/components/card",
      "../dev/raw/components/button",
      "../dev/raw/components/badge",
      "../dev/raw/components/backdrop",
      "../dev/raw/components/menus",

      "../dev/raw/components/grid",

      "../dev/raw/components/toolbar",
      "../dev/raw/components/navbar",
      "../dev/raw/components/scroll",
      "../dev/raw/components/region-picker",
      "../dev/raw/components/popover",
      "../dev/raw/components/picker",
      "../dev/raw/components/fab",
      "../dev/raw/components/datetime"
    ],
    "include": [
      "../README.md",
      "../dev/raw/components/scroll",
      "../dev/raw/components/fab",
      "../dev/raw/components/popover",
      "../dev/raw/components/picker",
      "../dev/raw/components/region-picker",
      "../dev/raw/components/datetime"
    ],
    "exclude": [
      "gulpfile.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true,
    "homeName": "首页 / Home",
    "links": [
      {
        "name": "如何开始 / How To Start",
        "link": "https://github.com/DTFE/vimo-start-kit"
      },
      {
        "name": "示例 / Demo",
        "link": "http://xiangsongtao.com/vimo"
      },
      {
        "name": "更新日志 / Change Log",
        "link": "https://github.com/DTFE/Vimo/blob/master/CHANGELOG.md"
      },
      {
        "name": "代码规范 / Code Standard",
        "link": "https://github.com/DTFE/vimo-code-style/blob/master/README.md"
      },
      {
        "name": "常见问题 / Question",
        "link": "#"
      }
    ]
  }
}

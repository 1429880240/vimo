{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "destination": "../docs",
    "encoding": "utf8",
    "template": "theme"
  },
  "plugins": [
    "node_modules/jsdoc-vue",
    "node_modules/jsdoc/plugins/markdown",
    "plugins/slot.js",
    "plugins/demo.js",
    "plugins/component.js"
  ],
  "markdown": {
    "tags": [
      "usage",
      "component",
      "slot"
    ],
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include": [
      "../README.md",
      "../dev/raw/components/app",
      "../dev/raw/components/content"
    ],
    "exclude": [
      "gulpfile.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}

{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-2"
  ],
  "comments": false,
  "plugins": [
    [
      "vimo",
      {
        "libraryName": "vimo",
        "libraryDirectory": "components",
        "style": "scss",
        "themes": [

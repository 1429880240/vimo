          {
            "themeName": "vimo-theme-md",
            "themeDirectory": "components",
            "themeExt": "scss",
            "symbol": "md"
          },
          {
            "themeName": "vimo-theme-wp",
            "themeDirectory": "components",
            "themeExt": "scss",
            "symbol": "wp"
          }
        ]
      }
    ]
  ]
